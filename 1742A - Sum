#include<bits/stdc++.h>
using namespace std;

int main(){
    int t; // no .of test cases
    cin>>t;

    while(t--){
        int a, b, c; 
        cin>>a>>b>>c;

        long long sum = 0;

        if(a == b+c || b == a+c || c == a+b){
            cout<<"YES"<<endl;
        }
        else{
            cout<<"NO"<<endl;
        }
    }
}
