#include<bits/stdc++.h>
using namespace std;

void solve(){
    int a, b;
    cin>>a>>b;

    if(a%b == 0){
        cout<<"0"<<endl;
        return;
    }
    if(a > b){
        cout<< (b - a%b) <<endl;
    }
    else{
        // if(a < b)
        cout<< (b - a) <<endl;
    }
}


int32_t main(){
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);

    int t;
    cin>>t;

    while(t--){
        solve();
    }

    return 0;
}
