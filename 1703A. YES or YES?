#include<bits/stdc++.h>

using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        string str;  // string 'str' of length 3
        cin>>str; // take the string from the user

        int n = str.length();

        if(str[0] == 'Y' || str[0] == 'y'){
            if(str[1] == 'E' || str[1] == 'e'){
                if(str[2] == 'S' || str[2] == 's'){
                    if(n == 3){
                        cout<<"YES"<<endl;
                    }
                    else{
                        cout<<"NO"<<endl;
                    }
                }
                else{
                    cout<<"NO"<<endl;
                }
            }
            else{
                cout<<"NO"<<endl;
            }
        }
        else{
            cout<<"NO"<<endl;
        }

    }
}
