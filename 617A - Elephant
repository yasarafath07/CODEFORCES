#include<bits/stdc++.h>
using namespace std;

int32_t main(){
    ios :: sync_with_stdio(NULL);
    cin.tie(NULL);
    cout.tie(NULL);

    int x; // coordinate of friends house
    cin>>x; 

    int steps = x / 5; 

    if(x % 5 != 0){
        steps++;
    }

    cout<<steps<<endl;

    return 0; 
}
