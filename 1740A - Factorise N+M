#include<bits/stdc++.h>
using namespace std;
 
/*
// Properties of PRIME NUMBERS:
- All Prime Numbers from 2 to n is odd except 2.
*/
 
int main(){
    int t;
    cin>>t;
 
    while(t--){
        int n;
        cin>>n;
 
        if(n == 2){
            cout<<"2"<<endl; // add 2 to n so that the resulting m is not prime
        }
        else{
            cout<<"3"<<endl; // multiple answers are possible
        }
    }
}
