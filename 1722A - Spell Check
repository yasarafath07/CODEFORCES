#include<bits/stdc++.h>
using namespace std;

int main(){
    int t;
    cin>>t;

    while(t--){
        int n; // length of the string s
        cin>>n;

        string s;
        cin>>s;

        string s1 = "Timur";

        sort(s.begin(), s.end());
        sort(s1.begin(), s1.end());

        if(s == s1){
            cout<<"YES"<<endl;
        }
        else{
            cout<<"NO"<<endl;
        }
    }
}
