#include<bits/stdc++.h>
using namespace std;

void solve(){
    long long n ; // length of the Array
    cin>>n;

    vector<long long> arr(n);
    long long prod = 1;

    for(int i=0; i<n; i++){
        cin>>arr[i];
        prod *= arr[i];
    }

    long long ans = prod + n - 1;

    cout<<2022*ans<<endl; 
}

int32_t main(){
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);

    long long t;
    cin>>t;

    while(t--){
        solve();
    }

    return 0;
}
