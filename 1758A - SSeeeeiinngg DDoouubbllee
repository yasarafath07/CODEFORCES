#include<bits/stdc++.h>
using namespace std;

void solve(){
    string s;
    cin>>s;

    string rev = s;

    // reverse the string 'rev'
    reverse(rev.begin(), rev.end());

    string ans = s + rev; // this will be a palindrome now

    cout<<ans<<endl;
}

int32_t main(){
    ios :: sync_with_stdio(NULL);
    cin.tie(NULL);
    cout.tie(NULL);

    int t;
    cin>>t;

    while(t--){
        solve();
    }

    return 0;
}
