#include<bits/stdc++.h>
using namespace std;

// My 1st Contest (Round #842 - Div 2)

void solve(){
    long long k;
    cin>>k;

    if(k >= 2){
        cout<<k-1<<endl;
        return;
    }
    cout<<-1<<endl;
}


int32_t main(){
    ios :: sync_with_stdio(NULL);
    cin.tie(NULL);
    cout.tie(NULL);

    long long t;
    cin>>t;

    while(t--){
        solve();
    }

    return 0;
}

// Always declare long long instead of int

