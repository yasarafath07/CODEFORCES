#include<bits/stdc++.h>
using namespace std;

#define ll long long

int32_t main(){
    ios_base::sync_with_stdio(false); // this disables synchronization between C and C++ standard streams
    cin.tie(NULL); // this unties cin from cout

    int t;
    cin>>t;

    while(t--){
        int n; // length of the array
        cin>>n;

        vector<int> arr(n); // vector of size n
        ll sum = 0;

        for(int i=0; i<n; i++){
            cin>>arr[i];
            sum += arr[i];
        }

        ll ans = abs(sum);

        cout<<ans<<endl;
    }

}
