#include<bits/stdc++.h>
using namespace std;

void solve(){
    int n; // length of the Array
    cin>>n;

    vector<int> arr(n); // int arr[n];

    for(int i=0; i<n; i++){
        cin>>arr[i];
    }

    if(arr[0] == 1){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
}


int32_t main(){
    ios :: sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    int t;
    cin>>t;

    while(t--){
        solve();
    }

    return 0;
}
